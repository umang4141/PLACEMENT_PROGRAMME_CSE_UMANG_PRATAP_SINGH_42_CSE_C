//package Arrays;
import java.util.*;
import java.io.*;
public class Givernprefix {
    
    public static void main(String args[])
    {
        String words[]={"pay","attention","practice","attend"};
        String pref = "at";
        int count = 0;
        for(String word:words)
        {
            if(word.startsWith(pref))
            {
                count++;
            }
        }
        System.out.println(count);
    }
}
