class Solution {
    public boolean squareIsWhite(String coordinates) {
        int val = (coordinates.charAt(0)-'a')+1;
        int num = (int)(coordinates.charAt(1));
        if((val%2!=0 && num%2!=0)||(val%2==0 && num%2==0)){
            
           return false;
        }
           else{
           return true;
    }
 }
}
