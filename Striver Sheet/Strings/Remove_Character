import java.util.*;
import java.io.*;
public class remove_character {
    public static void main(String args[])
    {
        String str1 = "xxyyzxx";
        char ch ='y';
        String str2 = "";
        for(int i=0;i<str1.length();i++)
{
    if(str1.charAt(i)!=ch)
 
        str2 = str2 + str1.charAt(i);
  
}
System.out.println(str2);
    }
}
